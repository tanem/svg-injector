{
  "extends": [
    "config:js-lib",
    ":rebaseStalePrs",
    ":automergeAll",
    ":label(internal)",
    ":masterIssue",
    ":enableVulnerabilityAlerts"
  ],
  "ignorePaths": [
    "**/node_modules/**"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "depTypeList": [
        "dependencies"
      ],
      "rangeStrategy": "bump"
    }
  ],
  "prConcurrentLimit": 1,
  "stabilityDays": 3
}